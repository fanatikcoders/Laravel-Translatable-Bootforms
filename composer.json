{
    "name": "propaganistas/laravel-translatable-bootforms",
    "description": "Empowers galahad/bootforms with astrotomic/laravel-translatable.",
    "keywords": ["laravel", "bootforms", "translatable", "i18n", "form"],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.0",
        "galahad/bootforms": "^1.1.2",
        "astrotomic/laravel-translatable": ">=5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelTranslatableBootForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelTranslatableBootForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelTranslatableBootForms\\TranslatableBootFormsServiceProvider"
            ],
            "aliases": {
                "TranslatableBootForm": "Propaganistas\\LaravelTranslatableBootForms\\Facades\\TranslatableBootForm"
            }
        }
    },
    "prefer-stable": true
}
